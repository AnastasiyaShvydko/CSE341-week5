<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel="stylesheet" href="/public/assets/style.css">
    <title>Document</title>
    <style>
        
    </style>
</head>
<body>

    <div id="container">
    <%- include('partials/navbar') %>
    <% data.forEach(function(dat) { %>
        
        <div class="card">
            <div class="card_title">
                <h1>
                    <%= dat.name %>
                </h1>
            </div>
            <div class="card_info">
                <div class="card_img">
                    <img  src = "<%= dat.image %>"" alt="" srcset="">
                </div>
                <ul class = "card_list">
                    <li>Mass: <%= dat.mass%></li>
                    <li>Radius: <%= dat.radius%></li>
                    <li>Period: <%= dat.period %></li>
                    <!-- <li><%= dat.semi_major_axis%></li>
                    <li><%= dat.temperature%></li>
                    <li><%= dat.distance_light_year %></li>
                    <li><%= dat.host_star_mass%></li>
                    <li><%= dat.host_star_temperature%></li> -->
                </ul>
             
            </div>
       <a class="btn" href="/planets/<%= dat._id%>">See More</a> 
        </div>
        
    <% }); %>
    <a class="btn" href="/planets/addView">Add New</a> 
    </div>
</body>
</html>